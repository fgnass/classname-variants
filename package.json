{
  "name": "classname-variants",
  "version": "1.4.0",
  "description": "Variant API for plain class names",
  "author": "Felix Gnass <fgnass@gmail.com>",
  "license": "MIT",
  "repository": "fgnass/classname-variants",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "type": "./lib/index.d.ts"
    },
    "./react": {
      "import": "./lib/react.js",
      "type": "./lib/react.d.ts"
    },
    "./preact": {
      "import": "./lib/preact.js",
      "type": "./lib/preact.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/index.d.ts"
      ],
      "react": [
        "./lib/react.d.ts"
      ],
      "preact": [
        "./lib/preact.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "start": "npx vite",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "tailwind",
    "css",
    "classname",
    "variants",
    "react",
    "preact"
  ],
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "preact": "^10.20.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.4"
  }
}
