{
  "name": "classname-variants",
  "version": "1.2.1",
  "description": "Variant API for plain class names",
  "author": "Felix Gnass <fgnass@gmail.com>",
  "license": "MIT",
  "repository": "fgnass/classname-variants",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "type": "./lib/index.d.ts"
    },
    "./react": {
      "import": "./lib/react.js",
      "type": "./lib/react.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/index.d.ts"
      ],
      "react": [
        "./lib/react.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "start": "npx vite",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "tailwind",
    "css",
    "classname",
    "variants",
    "react"
  ],
  "devDependencies": {
    "@types/react": "^17.0.39",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.6.4"
  }
}
